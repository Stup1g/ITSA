#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;

int main() {
    int n;
    double area, length;

    cin >> n;
    for (int i = 0; i < n; i++) {
        cin >> length;
        area = round(length * length * 10) / 10;
        cout << fixed << setprecision(1) << area << endl;
    }
    return 0;
}
