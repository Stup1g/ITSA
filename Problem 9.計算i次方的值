#include <iostream>
using namespace std;

int main() {
    int n;
    int i;
    cin >> n;

    for (int j = 0; j < n; j++) {
        int result = 1;

        cin >> i;
        if (i > 31) {
            cout << "Value of more than 31" << endl;
        } else {
            for (int k = 0; k < i; k++) {
                result *= 2;
            }
            cout << result << endl;
        }
    }
    return 0;
}
