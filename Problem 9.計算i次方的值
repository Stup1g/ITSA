#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    
    int i;
    for (int j = 0; j < n; j++) {
        int result = 1;
        scanf("%d", &i);
        
        if (i > 31) {
            printf("Value of more than 31\n");
        } else {
            for (int k = 0; k < i; k++) {
                result *= 2;
            }
            printf("%d\n", result);
        }
    }
    
    return 0;
}
