#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    int min;
    double bill;

    cin >> min;

    if (min <= 800) {
        bill = min * 0.9;
    } else if (min < 1500) {
        bill = min * 0.81;
    } else {
        bill = min * 0.711;
    }

    cout << fixed << setprecision(1) << bill << endl;

    return 0;
}
