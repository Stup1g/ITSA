#include <iostream>
using namespace std;

int main() {
    int num;
    cin >> num;

    int d1 = num / 10000; 
    int d2 = (num / 1000) % 10; 
    int d3 = (num / 100) % 10;  
    int d4 = (num / 10) % 10;   
    int d5 = num % 10;         

    for(int i = 0; i < d1; i++) cout << "*";
    cout << endl;

    for(int i = 0; i < d2; i++) cout << "*";
    cout << endl;

    for(int i = 0; i < d3; i++) cout << "*";
    cout << endl;

    for(int i = 0; i < d4; i++) cout << "*";
    cout << endl;

    for(int i = 0; i < d5; i++) cout << "*";
    cout << endl;

    return 0;
}
