#include <stdio.h>
#include <stdlib.h>

int main() {
    int num;
    scanf("%d", &num);
    
    int d1 = num / 10000; 
    int d2 = (num / 1000) % 10; 
    int d3 = (num / 100) % 10;  
    int d4 = (num / 10) % 10;   
    int d5 = num % 10;         
    
    for(int i = 0; i < d1; i++) printf("*");
    printf("\n");
    
    for(int i = 0; i < d2; i++) printf("*");
    printf("\n");
    
    for(int i = 0; i < d3; i++) printf("*");
    printf("\n");
    
    for(int i = 0; i < d4; i++) printf("*");
    printf("\n");
    
    for(int i = 0; i < d5; i++) printf("*");
    printf("\n");
    
    return 0;
}
